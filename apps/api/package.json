{
  "name": "@muzigal/api",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "jobs": "tsx src/jobs/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "echo \"No lint configured\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:seed": "tsx src/seed.ts",
    "db:seed:stages": "tsx src/seed-lead-stages.ts",
    "db:seed:instruments": "tsx src/seed-instruments.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "better-auth": "^1.4.18",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "express": "^4.19.2",
    "pg": "^8.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.24.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4"
  }
}
